{
  "expo": {
    "name": "week3",
    "slug": "week3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp", 
    "plugins": [
      "expo-router" 
    ],
    
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
      
    },
    "web": {
      "bundler": "metro", 
      "output": "static", 
      "favicon": "./assets/favicon.png"
    }
  }
}